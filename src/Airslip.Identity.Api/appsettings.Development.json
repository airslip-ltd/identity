{
    "PublicApiSettings": {
        "Base": {
            "BaseUri": "https://localhost:10289",
            "UriSuffix": "",
            "Version": "v1"
        },
        "BankTransactions": {
            "BaseUri": "https://localhost:22162",
            "UriSuffix": "",
            "Version": "v1"
        }
    },
    "JwtSettings": {
        "ExpiresTime": "1800"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Filter": [
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression": "Contains(RequestPath, '/status') and @Level in ['Information', 'Debug']"
                }
            }
        ]
    }
}

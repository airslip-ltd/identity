<Project Sdk="Microsoft.NET.Sdk.Web">

	<Import Project="..\common.props" />

	<ItemGroup>
		<PackageReference Include="JetBrains.Annotations" Version="$(JetBrainsAnnoations)" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.Google" Version="$(NetVersion)" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="$(NetVersion)" NoWarn="NU1605" />
		<PackageReference Include="Microsoft.Extensions.Http.Polly" Version="$(NetVersion)" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="$(NetVersion)" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc.Versioning" Version="$(NetVersion)" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc.Versioning.ApiExplorer" Version="$(NetVersion)" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="$(NetVersion)" />
		<PackageReference Include="Swashbuckle.AspNetCore" Version="$(Swashbuckle)" />
		<PackageReference Include="AspNetCore.Identity.MongoDbCore" Version="$(MongoDbIdentity)" />
		<PackageReference Include="Serilog.AspNetCore" Version="$(SerilogAspNetCore)" />
		<PackageReference Include="Serilog.Sinks.Logz.Io" Version="$(SerilogSinkLogzIo)" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Airslip.Common.Auth" Version="$(AirslipCommonAuth)" />
		<PackageReference Include="Airslip.Common.Middleware" Version="$(AirslipCommonMiddleware)" />
		<PackageReference Include="Airslip.Common.Contracts" Version="$(AirslipCommonContracts)" />
		<PackageReference Include="Airslip.Common.Monitoring" Version="$(AirslipCommonMonitoring)" />
		<PackageReference Include="Airslip.Common.Types" Version="$(AirslipCommonTypes)" />
		<PackageReference Include="Airslip.Email.Client" Version="$(AirslipEmailClient)" />
		<PackageReference Include="Airslip.Infrastructure.BlobStorage" Version="$(AirslipInfrastructureBlobStorage)" />
		<PackageReference Include="Airslip.Yapily.Client" Version="$(AirslipYapilyClient)" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Airslip.Common.Repository\Airslip.Common.Repository.csproj" />
		<ProjectReference Include="..\Airslip.Identity.Api.Application\Airslip.Identity.Api.Application.csproj" />
		<ProjectReference Include="..\Airslip.Identity.Api.Contracts\Airslip.Identity.Api.Contracts.csproj" />
		<ProjectReference Include="..\Airslip.Identity.AutoMapper\Airslip.Identity.AutoMapper.csproj" />
		<ProjectReference Include="..\Airslip.Identity.Infrastructure.MongoDb\Airslip.Identity.Infrastructure.MongoDb.csproj" />
		<ProjectReference Include="..\Airslip.Identity.MongoDb.Contracts\Airslip.Identity.MongoDb.Contracts.csproj" />
	</ItemGroup>

	<PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
		<DocumentationFile>bin\Debug\Airslip.Identity.Api.xml</DocumentationFile>
	</PropertyGroup>

	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
		<DocumentationFile>bin\Release\Airslip.Identity.Api.xml</DocumentationFile>
	</PropertyGroup>

</Project>
